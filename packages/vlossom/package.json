{
    "name": "vlossom",
    "description": "Vlossom UI components for Vue",
    "version": "0.0.0",
    "type": "module",
    "author": "PUBG Studio",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/pubg/vlossom.git"
    },
    "bugs": {
        "url": "https://github.com/pubg/vlossom/issues"
    },
    "license": "MIT",
    "keywords": [
        "vlossom",
        "vue",
        "components",
        "ui framework",
        "component library"
    ],
    "scripts": {
        "dev": "vite --port 3000",
        "build": "vue-tsc && vite build",
        "test": "vitest run --reporter=verbose",
        "test:watch": "vitest --reporter=verbose",
        "test:coverage": "vitest run --coverage",
        "test:snapshot": "vitest run --update",
        "lint": "eslint --ext .js,.ts,vue --ignore-path ../../.gitignore .",
        "lint:fix": "eslint --fix --ext .js,.ts,vue --ignore-path ../../.gitignore .",
        "storybook": "storybook dev -p 6006",
        "build-storybook": "storybook build",
        "test-storybook": "test-storybook --verbose",
        "chromatic": "chromatic --exit-zero-on-changes"
    },
    "files": [
        "dist",
        "src"
    ],
    "main": "./dist/vlossom.umd.cjs",
    "module": "./dist/vlossom.js",
    "types": "./dist/index.d.ts",
    "typings": "./dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/vlossom.js",
            "require": "./dist/vlossom.umd.cjs",
            "types": "./dist/index.d.ts"
        },
        "./styles": "./dist/style.css"
    },
    "peerDependencies": {
        "vue": "^3.4.0"
    },
    "devDependencies": {
        "@storybook/addon-a11y": "^7.6.10",
        "@storybook/addon-essentials": "^7.6.10",
        "@storybook/addon-interactions": "^7.6.10",
        "@storybook/addon-links": "^7.6.10",
        "@storybook/blocks": "^7.6.10",
        "@storybook/jest": "^0.2.3",
        "@storybook/test-runner": "^0.16.0",
        "@storybook/testing-library": "^0.2.2",
        "@storybook/vue3": "^7.6.10",
        "@storybook/vue3-vite": "^7.6.10",
        "@types/lodash-es": "^4.17.12",
        "@types/node": "^20.11.10",
        "@vitejs/plugin-vue": "^4.6.2",
        "@vitest/coverage-v8": "^1.2.2",
        "@vue/test-utils": "^2.4.4",
        "autoprefixer": "^10.4.17",
        "axe-playwright": "^1.2.3",
        "chromatic": "^10.6.1",
        "jsdom": "^23.2.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "ress": "^5.0.2",
        "sass": "^1.70.0",
        "storybook": "^7.6.10",
        "typescript": "^5.3.3",
        "vite": "^5.0.12",
        "vite-plugin-dts": "^3.7.2",
        "vitest": "^1.2.2",
        "vue": "^3.4.0",
        "vue-tsc": "^1.8.27"
    },
    "dependencies": {
        "@vueuse/core": "^10.8.0",
        "lodash-es": "^4.17.21",
        "nanoid": "^5.0.4",
        "vuedraggable": "^4.1.0"
    }
}
